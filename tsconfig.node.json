{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "target": "ES2020",
    "outDir": "./dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["server/**/*.ts",],
  "exclude": ["src/**/*", "node_modules"],
  "forceConsistentCasingInFileNames": true,

}
